{
  "name": "@clarigen/cli",
  "description": "A CLI for generating a Typescript interface for a Clarity contract.",
  "author": "Hank Stoever",
  "version": "0.2.7",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "oclif.manifest.json"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsdx watch",
    "start": "ts-node-script src/start.ts",
    "compile": "ncc build src/index.ts --no-source-map-register -e @oclif/command -e @oclif/errors",
    "build-single-file": "yarn compile && oclif-dev readme",
    "build": "rm -rf dist && tsc -b",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "clarigen": "./bin/run"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "clarigen"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/errors": "^1.3.4",
    "@vercel/ncc": "0.27.0",
    "oclif": "^1.16.1",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "@clarigen/core": "0.2.3",
    "@clarigen/native-bin": "0.2.7",
    "@ltd/j-toml": "1.12.2",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "3.2.3",
    "@stacks/wallet-sdk": "1.0.0-wallet-sdk.4",
    "chalk": "4.1.0",
    "chokidar": "3.5.1",
    "ora": "5.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "d3825e8cae60fc7a00bbd88a6abb84d1a5a7dfa6"
}
