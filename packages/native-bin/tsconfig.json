{
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  },
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "lib": ["dom", "esnext"],
    "outDir": "./dist",
    "rootDir": "./src",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["./src/**/*"]
}
